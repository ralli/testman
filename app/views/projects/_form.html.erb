<%= simple_form_for @project do |f| %>
  <%= f.error_notification %>
  <fieldset>
    <legend>Project</legend>
      <div class="fieldset">
        <%= f.input :name %>
        <%= f.button :submit %> <%= cancel_link @project, :class => 'button', :id => 'cancel' %>
      </div>
  </fieldset>
<% end %>