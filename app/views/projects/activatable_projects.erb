<% title "Activate Project"  %>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td> <%= link_to 'Activate', activate_project_url(project) %></td>
      </tr>
    <% end %>
  </tbody>
</table>